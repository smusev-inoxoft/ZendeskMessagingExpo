{
  "expo": {
    "name": "zendesk-messaging-expo-example",
    "slug": "zendesk-messaging-expo-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.zendeskmessagingexpo.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "expo.modules.zendeskmessagingexpo.example",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": [
              {
                "url": "https://zendesk.jfrog.io/artifactory/repo"
              }
            ]
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "aeb6c3b3-4604-404a-82ba-5375b7499810"
      }
    }
  }
}
